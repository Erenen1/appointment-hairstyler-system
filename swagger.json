    "/appointments": {
      "get": {
        "tags": [
          "Appointments"
        ],
        "summary": "Randevuları listele",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer",
              "default": 10
            }
          },
          {
            "name": "date",
            "in": "query",
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "staffId",
            "in": "query",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Randevular başarıyla listelendi",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiSuccessResponse"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Appointments"
        ],
        "summary": "Yeni randevu oluştur",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "customerName",
                  "customerPhone",
                  "appointmentDate",
                  "serviceId",
                  "staffId"
                ],
                "properties": {
                  "customerName": {
                    "type": "string",
                    "example": "Ahmet Yılmaz"
                  },
                  "customerPhone": {
                    "type": "string",
                    "example": "+90 555 123 4567"
                  },
                  "customerEmail": {
                    "type": "string",
                    "format": "email"
                  },
                  "appointmentDate": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "serviceId": {
                    "type": "integer",
                    "example": 1
                  },
                  "staffId": {
                    "type": "integer",
                    "example": 1
                  },
                  "notes": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Randevu başarıyla oluşturuldu",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiSuccessResponse"
                }
              }
            }
          }
        }
      }
    },
